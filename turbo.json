{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "API_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build",
        "generate"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "generate": {
      "outputs": [
        "src/definitions.ts"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "generate"
      ]
    },
    "test": {},
    "type-check": {
      "dependsOn": [
        "^type-check",
        "generate"
      ]
    }
  }
}