{
  "name": "vee",
  "private": true,
  "packageManager": "npm@10.9.4",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "test": "turbo test",
    "generate": "turbo run generate",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "type-check": "turbo type-check",
    "dc": "docker-compose -f docker/docker-compose.yml",
    "dc-prod": "docker-compose -f docker/docker-compose.production.yml"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "prettier-config-custom": "*",
    "turbo": "latest"
  },
  "prettier": "prettier-config-custom",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}